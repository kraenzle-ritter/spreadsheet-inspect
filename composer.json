{
    "name": "kraenzle-ritter/spreadsheet-inspect",
    "description": "CLI tool to inspect and analyze Excel/ODS spreadsheet files",
    "keywords": [
        "spreadsheet",
        "excel",
        "xlsx",
        "ods",
        "inspect",
        "analyze",
        "cli",
        "php"
    ],
    "homepage": "https://github.com/kraenzle-ritter/spreadsheet-inspect",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/kraenzle-ritter/spreadsheet-inspect/issues",
        "source": "https://github.com/kraenzle-ritter/spreadsheet-inspect"
    },
    "authors": [
        {
            "name": "Andreas Kr√§nzle",
            "email": "info@k-r.ch"
        }
    ],
    "require": {
        "php": "^8.3",
        "dompdf/dompdf": "^3.1",
        "laravel-zero/framework": "^12.0.2",
        "phpoffice/phpspreadsheet": "^5.4",
        "rap2hpoutre/fast-excel": "^5.6"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": [
        "inspect"
    ]
}
